{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "definitions": {
    "User": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "email": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "orders": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Order"
          }
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "Restaurant": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "address": {
          "type": "string"
        },
        "type": {
          "type": [
            "string",
            "null"
          ]
        },
        "products": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Product"
          }
        },
        "orders": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Order"
          }
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "Order": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "date": {
          "type": "string",
          "format": "date-time"
        },
        "user": {
          "$ref": "#/definitions/User"
        },
        "restaurant": {
          "$ref": "#/definitions/Restaurant"
        },
        "delivery": {
          "anyOf": [
            {
              "$ref": "#/definitions/Delivery"
            },
            {
              "type": "null"
            }
          ]
        },
        "products": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/OrderOfProducts"
          }
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "Product": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "name": {
          "type": "string"
        },
        "price": {
          "type": "number"
        },
        "description": {
          "type": [
            "string",
            "null"
          ]
        },
        "restaurant": {
          "$ref": "#/definitions/Restaurant"
        },
        "orders": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/OrderOfProducts"
          }
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "OrderOfProducts": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "price": {
          "type": "number"
        },
        "order": {
          "$ref": "#/definitions/Order"
        },
        "product": {
          "$ref": "#/definitions/Product"
        },
        "assignedAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    },
    "Delivery": {
      "type": "object",
      "properties": {
        "id": {
          "type": "integer"
        },
        "deliveryDate": {
          "type": [
            "string",
            "null"
          ],
          "format": "date-time"
        },
        "status": {
          "type": "string",
          "default": "IN_PROGRESS"
        },
        "address": {
          "type": "string"
        },
        "order": {
          "$ref": "#/definitions/Order"
        },
        "createdAt": {
          "type": "string",
          "format": "date-time"
        },
        "updatedAt": {
          "type": "string",
          "format": "date-time"
        }
      }
    }
  },
  "type": "object",
  "properties": {
    "user": {
      "$ref": "#/definitions/User"
    },
    "restaurant": {
      "$ref": "#/definitions/Restaurant"
    },
    "order": {
      "$ref": "#/definitions/Order"
    },
    "product": {
      "$ref": "#/definitions/Product"
    },
    "orderOfProducts": {
      "$ref": "#/definitions/OrderOfProducts"
    },
    "delivery": {
      "$ref": "#/definitions/Delivery"
    }
  }
